# Configuración de la API
REACT_APP_API_URL=http://localhost:8000/api

# Configuración de autenticación
REACT_APP_AUTH_TOKEN_KEY=authToken
REACT_APP_AUTH_EXPIRY_KEY=authExpiry

# Configuración de la aplicación
REACT_APP_TITLE=Core Personas
REACT_APP_VERSION=1.0.0

# Configuración de características
REACT_APP_ENABLE_AUDIT=true
REACT_APP_ENABLE_ANALYTICS=false

# Configuración de desarrollo
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Configuración de seguridad
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_ENABLE_DEMO_LOGIN=true

# Credenciales de demo (solo para desarrollo)
REACT_APP_DEMO_EMAIL=admin@auditoria.com
REACT_APP_DEMO_PASSWORD=admin123
